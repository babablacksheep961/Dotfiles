export BROWSER=/usr/bin/firefox
export EDITOR='nvim'
export STORAGE=$HOME/str
export STARSHIP_CONFIG=$ZSH_CONFIG/starship.toml
export DOT_DIR=$HOME/Dotfiles/config/
path=(
